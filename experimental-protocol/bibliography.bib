@article{turgay_identifying_2006,
	title = {Identifying the mechanical properties of tissue by ultrasound strain imaging},
	volume = {32},
	issn = {0301-5629},
	url = {https://www.sciencedirect.com/science/article/pii/S0301562905003789},
	doi = {10.1016/j.ultrasmedbio.2005.09.015},
	abstract = {A new elastography method is presented that images the mechanical properties of soft tissue. The tissue is externally vibrated over a range of frequencies simultaneously and the resulting displacement is recorded at multiple locations and time instants with a sequence of ultrasound images. Two methods are proposed for estimating the mechanical properties from the recorded data. In the first method, equations of motion are written for a tissue model of masses, springs and dampers. The equations are solved to identify the model parameters and construct an image. The second method performs a transfer function analysis of the tissue motion. The tissue properties are identified from the shape the of transfer function. Simulations show good performance of the new method compared with static elastography. Initial experimental results from homogeneous and layered tissue phantoms also demonstrate the ability quantitatively to image tissue stiffness. Preliminary results are obtained for viscosity and density estimation. Further work is needed to extend the formulation to 3-D and improve robustness of the viscosity and density estimates. (E-mail: tims@ece.ubc.ca)},
	number = {2},
	urldate = {2024-02-26},
	journal = {Ultrasound in Medicine \& Biology},
	author = {Turgay, Emre and Salcudean, Septimiu and Rohling, Robert},
	month = feb,
	year = {2006},
	keywords = {Density, Elastography, Mass-spring-damper model, Stiffness, Transfer function, Viscosity},
	pages = {221--235},
}

