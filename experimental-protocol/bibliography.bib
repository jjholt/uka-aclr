	urldate = {2024-04-15},
	journal = {The Knee},
	author = {Blevins, Jason L. and Carroll, Kaitlin M. and Burger, Joost A. and Pearle, Andrew D. and Bostrom, Mathias P. and Haas, Steven B. and Sculco, Thomas P. and Jerabek, Seth A. and Mayman, David J.},
	month = mar,
	year = {2020},
	keywords = {Early postoperative outcomes, Forgotten joint, Return to work, Total knee arthroplasty, Unicompartmental arthroplasty},
	pages = {565--571},
}

